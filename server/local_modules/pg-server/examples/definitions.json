[{
  "database": {
    "public": {
      "tenant": {
        "columns": {
          "tenant_id": "uuid",
          "date_created": "timestamp",
          "date_modified": "timestamp",
          "name": "text"
        },
        "primaryKey": [
          "tenant_id"
        ]
      }
    },
    "tenant-1": {
      "client": {
        "columns": {
          "client_id": "uuid",
          "date_created": "timestamp",
          "date_modified": "timestamp",
          "name": "text",
          "special_key_1": "number"
        },
        "primaryKey": [
          "client_id"
        ]
      },
      "project": {
        "columns": {
          "project_id": "uuid",
          "date_created": "timestamp",
          "date_modified": "timestamp",
          "name": "text",
          "client_id": "uuid"
        },
        "primaryKey": [
          "project_id"
        ]
      }
    },
    "tenant-2": {
      "client": {
        "columns": {
          "client_id": "uuid",
          "date_created": "timestamp",
          "date_modified": "timestamp",
          "name": "text",
          "special_key_2": "number"
        },
        "primaryKey": [
          "client_id"
        ]
      }
    }
  },
  "examples": [{
    "query": {
      "qt": "insert",
      "into": "tenant",
      "columns": [
        "date_created",
        "date_modified",
        "name"
      ],
      "values": "{{values body}}"
    },
    "incoming": {
      "body": [{
        "name": "My Name"
      }, {
        "name": "My Other Name",
        "date_created": "2018-06-02"
      }]
    }
  }, {
    "query": {
      "qt": "insert",
      "into": "\"tenant-{{query.tenant_id}}\".client",
      "values": "{{values body}}"
    },
    "incoming": {
      "query": {
        "tenant_id": 2
      },
      "body": {
        "name": "George Orwell",
        "special_key_1": 11,
        "special_key_2": 22
      }
    }
  }, {
    "query": {
      "qt": "each",
      "path": "body",
      "query": {
        "qt": "insert",
        "with": {
          "alias": "cl",
          "query": {
            "qt": "insert",
            "into": "\"tenant-{{$parentContext.query.tenant_id}}\".client",
            "values": "{{values @root}}",
            "returning": "client_id"
          }
        },
        "into": "\"tenant-{{$parentContext.query.tenant_id}}\".project",
        "values": "{{values projects}}"
      }
    },
    "incoming": {
      "query": {
        "tenant_id": 1
      },
      "body": [{
        "name": "George Orwell",
        "special_key_2": 22,
        "projects": {
          "name": "1984"
        }
      }, {
        "name": "George Lucas",
        "special_key_2": 78,
        "projects": [{
          "name": "Star Wars"
        }, {
          "name": "Indiana Jones"
        }]
      }]
    }
  }]
}]